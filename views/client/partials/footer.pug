.container
  .footer 
    .row
      .col-4
        .inner-left
          h3 Cơ sở chính
          ul
            li
              i.fa-solid.fa-location-dot
              h4 Địa chỉ: #{settingGeneral.addressText}
            li
              i.fa-brands.fa-tiktok
              p Tik tok: #{settingGeneral.tiktok}
            li
              i.fa-solid.fa-phone
              p Bảo hành: #{settingGeneral.phone}
            li
              i.fa-duotone.fa-solid.fa-envelope
              p Email: #{settingGeneral.email}
            li
              i.fa-duotone.fa-solid.fa-clock
              p Thời gian mở cửa: 8h - 21h

      .col-4
        .inner-center
          h3 Chính sách cửa hàng
          ul
            li Chính sách bảo hành
            li Chính sách thanh toán
            li Chính sách đổi trả
            li Chính sách bảo mật
            li Chính sách vận chuyển
            li Chính sách vệ sinh

      .col-4
        .innetr-right 
          h3 Bảng đồ
          iframe(
            src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d19750.351279445687!2d106.59446322984623!3d10.813912107304002!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x31752b0ba1226c2b%3A0x491f5422aeaaa5a4!2zNTAgxJDGsOG7nW5nIHPhu5EgNSwgQsOsbmggSMawbmcgSG_DoCwgQsOsbmggVMOibiwgSOG7kyBDaMOtIE1pbmgsIFZp4buHdCBOYW0!5e0!3m2!1svi!2s!4v1731250392390!5m2!1svi!2s"
            width="100%" height="200px" style="border:0; border-radius: 8px;" allowfullscreen="" loading="lazy"
            referrerpolicy="no-referrer-when-downgrade")
        

  .copy #{settingGeneral.copyright}
